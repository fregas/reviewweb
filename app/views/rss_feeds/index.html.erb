<h1>Listing rss_feeds</h1>

<table>
  <tr>
  </tr>

<% @rss_feeds.each do |rss_feed| %>
  <tr>
    <td><%= link_to 'Show', rss_feed %></td>
    <td><%= link_to 'Edit', edit_rss_feed_path(rss_feed) %></td>
    <td><%= link_to 'Destroy', rss_feed, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

